{
  "name": "webvi-websockets",
  "version": "3.2.0",
  "description": "WebSocket wrappers for LabVIEW NXG WebVI JSLI nodes",
  "main": "source/main.js",
  "scripts": {
    "start-server": "node test/test-servers/multitestserver.js",
    "test": "karma start --browsers ChromeHeadlessNoSandbox && karma start --browsers FirefoxHeadless --skip-tags FailsFirefox",
    "test-dev": "karma start --browsers ChromeHeadlessNoSandbox --skip-tags Slow",
    "test-manual": "karma start --no-single-run",
    "test-coverage": "karma start karma.coverage.conf.js",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ni-kismet/webvi-websockets.git"
  },
  "keywords": [
    "LabVIEW",
    "NXG",
    "WebSockets",
    "JSLI"
  ],
  "author": "National Instruments",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ni-kismet/webvi-websockets/issues"
  },
  "homepage": "https://github.com/ni-kismet/webvi-websockets#readme",
  "devDependencies": {
    "eslint": "^6.8.0",
    "jasmine-core": "^3.5.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.1",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^3.0.3",
    "karma-jasmine-html-reporter": "^1.5.1",
    "karma-jasmine-matchers": "^4.0.2",
    "karma-jasmine-spec-tags": "^1.2.0",
    "karma-verbose-reporter": "0.0.6",
    "puppeteer": "^2.0.0",
    "ws": "^7.2.1"
  }
}
